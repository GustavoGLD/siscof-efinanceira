                                        <xs:simpleType>
                                          <xs:restriction base="xs:string"/>
                                        </xs:simpleType>
                                      </xs:element>
                                      <xs:element name="CaixaPostal" minOccurs="0">
                                        <xs:simpleType>
                                          <xs:restriction base="xs:string"/>
